    "yaml-server": "^1.10.0"
  },
  "devDependencies": {
    "gh-pages": "^6.3.0",
    "newman": "^6.2.1",
    "newman-reporter-htmlextra": "^1.23.1",
    "wait-on": "^9.0.1"
  }
}
